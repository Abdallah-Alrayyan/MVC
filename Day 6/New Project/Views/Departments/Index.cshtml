@model IEnumerable<New_Project.Department>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                DepartmentName
            </th>
            <th>
               Location
            </th>
            <th>
              ManagerId
            </th>
            <th>
                CreatedAt
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @item.DepartmentName
            </td>
            <td>
                @item.Location
            </td>
            <td>
                @item.ManagerId
            </td>
            <td>
                @item.CreatedAt
            </td>
         
        </tr>
}
    </tbody>
</table>
